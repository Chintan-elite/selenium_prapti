package collection;

import java.util.ArrayList;

public class C001_ArraylistDemo {
	public static void main(String[] args) {
		
		
		ArrayList<String> al = new ArrayList<>();
		al.add("java");
		al.add("selenium");
		al.add("php");
		al.add("Android");
		
		//al.add(1,"Testing");
		//al.set(1, "testing");
		//al.remove(2);
		System.out.println(al.get(3));
		
		//System.out.println(al.size());
		
		for(String s : al)
		{
			System.out.println(s);
		}
		
		
		
		
		
		
		
		
	}
}
