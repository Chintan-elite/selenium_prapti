package fileio;

import java.io.File;

public class F005_FileDemo {
	public static void main(String[] args) {
		
		
		
		File file = new File("D://test//data.txt");
		
		if(file.exists())
		{
			System.out.println("File awailable !!!");
		}else
		{
			System.out.println("File does not exist !!!");
		}
		
		
		
		
		
	}
}
