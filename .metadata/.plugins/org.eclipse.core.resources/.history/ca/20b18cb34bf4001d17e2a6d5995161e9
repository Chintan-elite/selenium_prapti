package oops;

class Calc
{
	public void sample()
	{
		System.out.println("Running sample method...");
	}
	
	public void add(int a, int b)
	{
		int r = a+b;
		System.out.println("Additon is : "+r);
	}
}

public class O005_MethodManipulation {
	public static void main(String[] args) {
		
		Calc c = new Calc();
		c.sample();
		c.add(10, 20);
		
		
		
	}
}
