package array;

class Std
{
	private int id;
	private String name;
	
	public void setData(int id, String name)
	{
		id = id;
		name=name;
	}
	public void display()
	{
		System.out.println(id+" "+name);
	}
}

public class O006_EncaptulationDemo {
	public static void main(String[] args) {
		
		
		Std st = new Std();
		st.setData(10, "test");
		st.display();
		
		
	}
}
